				"import torch\n",
				"from datasets import load_dataset\n",
				"from torch.utils.data import DataLoader\n",
				"\n",
				"# distilBERT 토크나이저 로드\n",
				"tokenizer = torch.hub.load('huggingface/pytorch-transformers', 'tokenizer', 'distilbert-base-uncased')"
			]
